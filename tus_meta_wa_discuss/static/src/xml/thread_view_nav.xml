<?xml version="1.0" encoding="UTF-8"?>
<templates xml:space="preserve">

    <t t-name="tus_meta_whatsapp_base.ThreadViewNav" owl="1">
        <div class="o_thread_nav" t-if="threadViewNav and threadViewNav.channel and threadViewNav.channel.channel_type == 'chat'">
            <nav class="navbar navbar-expand-lg navbar-mainbg">
                      <div class="collapse navbar-collapse" id="navbarSupportedContent">
                        <ul class="navbar-nav pl-4">
                            <div class="hori-selector"><div class="left"></div>
                                <div class="right"></div></div>
                            <li class="nav-item live_chat_tab" t-if="threadViewNav and threadViewNav.channel and threadViewNav.channel.correspondent and threadViewNav.channel.correspondent.persona.partner and threadViewNav.channel.correspondent.persona.partner.user" t-att-class="{ 'active': threadViewNav and threadViewNav.isWaMsgs == false }">
                                <a class="nav-link live_chat current" href="javascript:void(0);" t-on-click="onClickLive">Live chat </a>
                            </li>
                            <li class="nav-item whatsup_chat_tab" t-att-class="{ 'active': threadViewNav and threadViewNav.isWaMsgs }">
                                <a class="nav-link whatsup_chat" href="javascript:void(0);" t-on-click="onClickWhatsapp">WhatsApp Chat </a>
                            </li>


                        </ul>
                    </div>

                </nav>

        </div>
    </t>
</templates>
